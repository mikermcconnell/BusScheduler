    "test:workflow-integration": "jest --config jest.config.workflow.js --testPathPattern='workflowPersistence\\.test\\.ts$'",
    "test:workflow-performance": "jest --config jest.config.workflow.js --testPathPattern='workflowPersistence\\.performance\\.test\\.ts$'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
