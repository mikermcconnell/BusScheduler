    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:vitest": "vitest",
    "typecheck": "tsc --noEmit",
    "precommit": "npm run typecheck && npm run build",
    "ci-check": "tsc --noEmit --strict",
    "type-coverage": "echo \"Type coverage check - manual for now\"",
